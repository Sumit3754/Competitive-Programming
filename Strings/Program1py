

T = int(input("Enter number of strings: "))

for _ in range(T):
    s = input("Enter string: ")
    vowels = 0
    consonants = 0

    for ch in s:
        if ch.isalpha():  
            if ch.lower() in 'aeiou':
                vowels += 1
            else:
                consonants += 1

    print("Vowels:", vowels, "Consonants:", consonants)
